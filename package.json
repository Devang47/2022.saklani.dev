		"@typescript-eslint/parser": "^5.9.1",
		"autoprefixer": "^10.4.2",
		"cssnano": "^5.0.15",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"husky": "^7.0.4",
		"p5": "^1.4.0",
		"postcss": "^8.4.5",
		"postcss-import": "^14.0.2",
		"postcss-nesting": "^10.1.2",
